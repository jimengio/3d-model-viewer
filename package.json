{
  "name": "@jimengio/3d-model-viewer",
  "version": "0.2.0",
  "description": "threejs-based 3D model viewer used in JimengIO",
  "main": "./lib/index.js",
  "scripts": {
    "dev": "cp -vr data webpack/ && webpack-dev-server --hot --config webpack/dev.js",
    "watch": "cp -vr data webpack/ && webpack-dev-server --hot --config webpack/dev.js",
    "dll": "rm -rfv webpack/dll/* && time webpack --config webpack/dll-dev.js",
    "dll-release": "time webpack --config webpack/dll-release.js",
    "release": "rm -rfv dist/* && yarn dll-release && time webpack --config webpack/release.js && cp -v data/* dist/",
    "build": "yarn release",
    "compile": "rm -rfv lib/* && tsc -d --project tsconfig-compile.json --outDir lib/",
    "upload": "rsync -avr --progress dist/ fe.jimu.io:/web-assets/repo/jimengio/3d-model-viewer",
    "gen-router": "ts-node -P tsconfig-node.json -r tsconfig-paths/register example/gen-router.ts",
    "postinstall": "rm -rfv node_modules/@types/*/node_modules/@types/react"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@jimengio/ci-progress-webpack-plugin": "^0.1.0",
    "@jimengio/doc-frame": "^0.1.12",
    "@jimengio/flex-styles": "^0.1.6",
    "@jimengio/router-code-generator": "^0.2.6",
    "@jimengio/ruled-router": "^0.2.25",
    "@types/node": "^13.11.0",
    "@types/prettier": "^2.0.0",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "cache-loader": "^4.1.0",
    "css-loader": "^3.5.1",
    "dayjs": "^1.8.23",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "emotion": "^10.0.27",
    "file-loader": "^6.0.0",
    "font-awesome": "^4.7.0",
    "fork-ts-checker-webpack-plugin": "^4.1.2",
    "html-webpack-plugin": "^4.0.4",
    "html-webpack-tags-plugin": "^2.0.17",
    "immer": "^6.0.3",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.0.4",
    "query-string": "^6.12.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "style-loader": "^1.1.3",
    "thread-loader": "^2.1.3",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.8.3",
    "typescript-styled-plugin": "^0.15.0",
    "url-loader": "^4.0.0",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-hud": "^0.1.2"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "dependencies": {
    "three": "^0.115.0",
    "use-immer": "^0.4.0"
  }
}
